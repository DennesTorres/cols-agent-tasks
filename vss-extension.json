{
    "manifestVersion": 1,
    "id": "DennesTorres-devOpsTasks",
    "name": "Dennes Torres' devOps Tasks",
    "version": "2.5.0",
    "publisher": "dennestorres",
    "public": true,
    "repository": {
        "type": "git",
        "uri": "https://github.com/colindembovsky/cols-agent-tasks"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "These devOps tasks will make SQL Server database devOps easier using SSDT",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "dacpac", "SSDT"
    ],
    "screenshots": [
        {
            "path": "images/ss_DacPacChangeReport.png"
        }
    ],
    "content": {
        "details": {
            "path": "extension-overview.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "links": {
        "home": {
            "uri": "http://dennestorres.com"
        },
        "repository": {
            "uri": "https://github.com/colindembovsky/cols-agent-tasks"
        },
        "issues": {
            "uri": "https://github.com/colindembovsky/cols-agent-tasks/issues"
        }
    },
    "branding": {
        "color": "rgb(36, 43, 50)",
        "theme": "dark"
    },
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "Tasks/DacPacReport"
        }
    ],
    "contributions": [
        {
            "id": "dacpac-report-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/DacPacReport"
            }
        }
    ]
}